{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": ["ec2:StartInstances",
            "ec2:StopInstances",
            "ec2:Describe*",
            "ec2:Get*",
            "ec2:List*",
            "elasticloadbalancing:Describe*",
            "ec2:TerminateInstances"],
            "Resource": "*"
        }
    ] 
}